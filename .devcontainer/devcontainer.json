{
    "name": "TestContainers Swift 6.2",
    "image": "swift:6.2-jammy",
    "customizations": {
        "vscode": {
            "extensions": [
                "swiftlang.swift-vscode",
                "vadimcn.vscode-lldb"
            ],
            "settings": {
                "lldb.library": "/usr/lib/liblldb.so"
            }
        }
    },
    "remoteUser": "vscode",
    "features": {
        "ghcr.io/devcontainers/features/common-utils:2": {
            "installZsh": "true",
            "username": "vscode",
            "userUid": "1000",
            "userGid": "1000",
            "upgradePackages": "true"
        },
        "ghcr.io/devcontainers/features/docker-in-docker:2.14.0": {
            "version": "latest",
            "moby": true
        }
    }
}